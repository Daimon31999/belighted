    "gatsby-plugin-postcss": "^2.3.11",
    "pure-react-carousel": "^1.27.3",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-tooltip": "^4.2.9",
    "tailwindcss": "^1.8.7"
  },
  "devDependencies": {
    "prettier": "2.1.1"
